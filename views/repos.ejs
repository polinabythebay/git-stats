<p>Username: <%= user %></p>

 <% if (repos) { %>
<table>
<% for(var key in repos) { %>
   <tr>
     <td><%= key %></td>
     <td><%= repos[key] %></td>
   </tr>
<% } %>
</table>
<% } %>

<!-- <button class="barChart">Bar Chart</button>
<div class="title"></div> -->

<a href="#" ng-click="barChart = !barChart" ng-class="{ active: barChart }">
  Show bar chart
</a>

<div ng-show='barChart' ng-controller="LineController as LineCtrl">
  <p> Overview <p>
  <canvas id="bar" class="chart chart-bar"
    chart-data="data" chart-labels="labels" chart-series="series">
  </canvas>
</div>
